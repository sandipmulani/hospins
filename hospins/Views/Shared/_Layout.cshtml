        /* Data Table css */
        .table-inner {
            padding: 30px;
        }

        .dataTables_wrapper table.dataTable {
            border-collapse: collapse;
            border: 0px;
        }

            .dataTables_wrapper table.dataTable thead th {
                padding: 8px 20px 8px 10px;
                font-weight: 500;
                border-top: 0px;
                border-bottom: 0px;
                white-space: nowrap;
                vertical-align: middle;
                -webkit-box-sizing: border-box;
                box-sizing: border-box;
            }

                .dataTables_wrapper table.dataTable thead th.sorting_disabled {
                    padding-right: 10px;
                }

            .dataTables_wrapper table.dataTable tbody th {
                border-top: 0px;
                white-space: nowrap;
            }

            .dataTables_wrapper table.dataTable tbody td {
                white-space: nowrap;
                vertical-align: middle;
                -webkit-box-sizing: border-box;
                box-sizing: border-box;
                border-top: 0px;
            }

            .dataTables_wrapper table.dataTable tfoot th {
                border-top: 0px;
                white-space: nowrap;
                vertical-align: middle;
                -webkit-box-sizing: border-box;
                box-sizing: border-box;
            }

            .dataTables_wrapper table.dataTable tfoot td {
                border-top: 0px;
                white-space: nowrap;
                vertical-align: middle;
                -webkit-box-sizing: border-box;
                box-sizing: border-box;
            }

        .dataTables_wrapper .dataTables_paginate .paginate_button {
            border-radius: 0px;
            margin-left: 0;
            padding: 5px 10px;
        }

        .dataTables_wrapper .dataTables_paginate {
            border: 1px solid #e1e8ef;
            padding: 0;
        }

            .dataTables_wrapper .dataTables_paginate .paginate_button.current {
                color: #ffffff !important;
                background: #428bca;
                border-color: #428bca;
            }

                .dataTables_wrapper .dataTables_paginate .paginate_button.current:hover {
                    color: #ffffff !important;
                    background: #428bca;
                    border-color: #428bca;
                }

            .dataTables_wrapper .dataTables_paginate .paginate_button {
                border-radius: 0px;
                margin-left: 0;
            }

                .dataTables_wrapper .dataTables_paginate .paginate_button:hover {
                    color: #ffffff !important;
                    background: #428bca;
                    border-color: #428bca;
                }

        .table-bordered td, .table-bordered th {
            border: 1px solid #dee2e6 !important;
        }

        /* Data Table End */
        /* Other dataTable CSS Start */
        .table .thead-dark th {
            background: #ffffff;
            color: #333;
        }

        .dataTables_wrapper.no-footer .dataTables_scrollBody {
            border-bottom: 0px;
        }

        .DTFC_LeftBodyLiner,
        .DTFC_RightBodyLiner {
            overflow: hidden;
            height: 0;
        }

        .DTFC_RightWrapper .DTFC_RightHeadWrapper {
            box-shadow: -7px 0 7px -2px rgba(0, 0, 0, 0.1);
            -moz-box-shadow: -7px 0 7px -2px rgba(0, 0, 0, 0.1);
            -webkit-box-shadow: -7px 0 7px -2px rgba(0, 0, 0, 0.1);
        }

        .DTFC_RightWrapper .DTFC_RightBodyWrapper {
            box-shadow: -6px 0 6px 0px rgba(0, 0, 0, 0.1);
            -moz-box-shadow: -6px 0 6px 0px rgba(0, 0, 0, 0.1);
            -webkit-box-shadow: -6px 0 6px 0px rgba(0, 0, 0, 0.1);
            z-index: 1;
        }

        .DTFC_LeftWrapper .DTFC_LeftHeadWrapper {
            box-shadow: 7px 0 7px 2px rgba(0, 0, 0, 0.1);
            -moz-box-shadow: 7px 0 7px 2px rgba(0, 0, 0, 0.1);
            -webkit-box-shadow: 7px 0 7px 2px rgba(0, 0, 0, 0.1);
        }

        .DTFC_LeftWrapper .DTFC_LeftBodyWrapper {
            box-shadow: 6px 0 6px 0px rgba(0, 0, 0, 0.1);
            -moz-box-shadow: 6px 0 6px 0px rgba(0, 0, 0, 0.1);
            -webkit-box-shadow: 6px 0 6px 0px rgba(0, 0, 0, 0.1);
            background: #f3f3f3;
            z-index: 1;
        }

        .dataTable .checkbox-fade {
            margin-right: 0px;
            display: block;
            height: 20px;
        }

            .dataTable .checkbox-fade .cr {
                margin: 0;
                float: none;
            }

            .dataTable .checkbox-fade label span {
                display: block;
            }

        /* Other dataTable CSS End */
        .toolbar {
            float: left !important;
        }

        .padd-left-right-0 {
            padding-left: 0px !important;
            padding-right: 0px !important;
        }

        /* .table.dataTable thead tr th {color: #ffffff;background-color: #343a40; border-color: #454d55;} */
        .w-48 {
            width: 48%;
        }

        .normal-right {
            float: right;
        }

        .normal-left {
            float: left;
        }

        /* datatable action button CSS Start*/
        .dataTables_info {
            width: 33.33%;
        }

        .toolbar {
            width: 33.33%;
            text-align: center;
            margin-top: 16px;
        }

        .content-wrap ul li.mt-checkbox-list {
            margin: 5px 0px;
        }

        .pdfbutton {
            margin-top: 25px;
        }

            .pdfbutton a + a {
                margin-right: 5px;
            }

        .font-16 {
            font-size: 16px;
        }

        /* After Sales project css */
        table.dataTable > tbody > tr.child {
            background-color: #ffffff;
        }

        table.dataTable.dtr-inline.collapsed > tbody > tr > td:first-child:before,
        table.dataTable.dtr-inline.collapsed > tbody > tr > th:first-child:before {
            top: calc(50% - 18px);
            transform: translateY(50%);
        }

        .column-selection {
            max-height: 148px;
            width: 220px;
            overflow-y: auto;
            margin: 0px !important;
        }

            .column-selection li {
                margin: 8px !important;
            }

        .toolbar .dropup .column-selection li .checkbox-fade label .checkbox-content {
            padding-left: 6px;
            font-size: 12px;
        }

        .dataTables_scrollBody {
            z-index: 1;
        }

        .dataTables_wrapper .dataTables_info {
            padding-top: 20px;
        }

        .dataTables_wrapper .dataTables_paginate {
            margin-top: 10px !important;
        }
        /* datatable action button CSS End*/


